{
  "expo": {
    "name": "Birthday Reminder",
      "slug": "birthday-reminder",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "birthday-reminder-app",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.skornakrystof.birthdayreminder",
      "infoPlist": {
        "NSContactsUsageDescription": "This app needs access to your contacts to import birthday information."
      }
    },
    "web": {
      "bundler": "metro",
      "output": "single",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      "expo-font",
      "expo-contacts",
      [
        "react-native-google-mobile-ads",
        {
          "iosAppId": "ca-app-pub-3940256099942544~1458002511"
        }
      ],
      [
        "react-native-cloud-storage",
        {
          "iCloudContainerIdentifier": "iCloud.com.skornakrystof.birthdayreminder",
          "iCloudContainerEnvironment": "Production"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "android": {
      "package": "com.yourcompany.birthdayreminder",
      "permissions": [
        "READ_CONTACTS"
      ]
    }
  }
}
